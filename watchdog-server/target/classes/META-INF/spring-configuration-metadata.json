{
  "groups": [
    {
      "name": "watchdog.server",
      "type": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean"
    }
  ],
  "properties": [
    {
      "name": "watchdog.server.eviction-interval-timer-in-ms",
      "type": "java.lang.Long",
      "description": "服务失效剔除任务轮询间隔时间",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": 0
    },
    {
      "name": "watchdog.server.lease-expiration-remove-duration-in-seconds",
      "type": "java.lang.Integer",
      "description": "发现失效后，多长时间remove 剔除",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": 30
    },
    {
      "name": "watchdog.server.persistence-enabled",
      "type": "java.lang.Boolean",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": false
    },
    {
      "name": "watchdog.server.persistence-type",
      "type": "java.lang.String",
      "description": "持久化策略，默认是 database",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": "database"
    },
    {
      "name": "watchdog.server.rmi-name",
      "type": "java.lang.String",
      "description": "RMI名，默认是 watchDogRMIService",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": "watchDogRMIService"
    },
    {
      "name": "watchdog.server.rmi-port",
      "type": "java.lang.Integer",
      "description": "RMI端口，默认是1099",
      "sourceType": "org.donglight.watchdog.server.config.WatchDogServerConfigBean",
      "defaultValue": 1099
    }
  ],
  "hints": []
}